{# Modal buscar Movimiemtos de Dispensers (modalBuscarMovimDisp)  #}

		<!-- Modal -->
		<div class="modal fade" id="modalBuscarMovimDisp" tabindex="-1" role="dialog" aria-labelledby="modalBuscarMovimDisp" aria-hidden="true">
		  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
		    <div class="modal-content">
		        <div class="modal-header">
		        	<h5 class="modal-title" id="exampleModalCenterTitle"><strong>Buscar movimientos de dispensers</strong></h5>
		        	<button id="btnModBusVisita" type="button" class="close" data-dismiss="modal" aria-label="Close">
		          		<span aria-hidden="true">&times;</span>
		        	</button>
		        </div>

		        <div class="modal-body" id="filter-2-container">

		        	<div class="row mt-2">
		        		<div class="col-1"></div>
						<div class="col-4 d-inline-flex">
						    <label class="lblFecha pr-2 pt-1" for="fechaDesde">Desde</label>
						    <input type="date" class="form-control form-control-sm" id="fechaDesde" value="{{ fechaDesde }}">
						</div>
						<div class="col-4 d-inline-flex">
						    <label class="lblFecha pr-2 pt-1" for="fechaHasta">Hasta</label>
						    <input type="date" class="form-control form-control-sm" id="fechaHasta" value="{{ fechaHasta }}">
						</div>
						<div class="col mb-2 d-flex">
							<button id="btnActualizar" type="button" class="btn btn-success btn-sm btnModal">Actualizar</button>
						</div>
		        	</div>

		        	{# BUSCAR MOVIM DISP #}
					<div class="row buscarMovDisp" id="radioBtsBuscarMovim">
					    <div class="col-3 mb-1">
					        <input id="filter" class="form-control form-control-sm" type="text" placeholder="Buscar por =>" autofocus>
					    </div>
					    <div class="col-9 mb-1">
					        <div class="custom-control custom-radio custom-control-inline">
					            <input class="custom-control-input" name="filtros" type="radio" id="radInterno" value="cellInterno" checked>
					            <label class="custom-control-label" for="radInterno">Nro. Interno</label>
					        </div>
					        <div class="custom-control custom-radio custom-control-inline">
					            <input class="custom-control-input" name="filtros" type="radio" id="radEmpl" value="cellEmpl">
					            <label class="custom-control-label" for="radEmpl">Empleado</label>
					        </div>
					        <div class="custom-control custom-radio custom-control-inline">
					            <input class="custom-control-input" name="filtros" type="radio" id="radClie" value="cellClie">
					            <label class="custom-control-label" for="radClie">Cliente</label>
					        </div>
					        <div class="custom-control custom-radio custom-control-inline">
					            <input class="custom-control-input" name="filtros" type="radio" id="radEstado" value="cellEstado">
					            <label class="custom-control-label" for="radEstado">Estado</label>
					        </div>
					    </div>
					</div>

					<div class="row tablaDispenser">
					    <div class="col">
					        <table class="table table-sm table-hover table-scroll small-first-col" id="tablaBuscarMovsDisp">
					            <thead class="thead-dark">
					                <tr>
					                    <th scope="col" style="display: none;">Id</th>
					                    <th scope="col">Fecha</th>
					                    {# <th scope="col">Nro. Serie</th> #}
					                    <th scope="col">Nro. Interno</th>
					                    <th scope="col">Modelo</th>
					                    <th scope="col">Empleado</th>
					                    <th scope="col">Cliente</th>
					                    <th scope="col">Estado</th>
					                </tr>
					            </thead>
								<tbody id="bodyTablaModalBuscar" class="body-half-screen">
				                	{# Esto es devuelto por ajax !! #}
					            </tbody>
					        </table>
					        <hr>
					    </div>  {# /div.col #}
					</div>  {# /.row #}

				</div>  {# /.modal-body #}

		        <div class="modal-footer">
		        	<button id="btnCerModBusVis" type="button" class="btn btn-outline-secondary" data-dismiss="modal">Cerrar</button>
		        </div>
		      </div>
		    </div>
		</div>
